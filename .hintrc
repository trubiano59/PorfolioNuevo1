{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "listitem": "off",
        "list": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "mask-image",
          "scroll-behavior"
        ]
      }
    ],
    "axe/forms": "off",
    "no-inline-styles": "off",
    "highest-available-document-mode": "off",
    "axe/aria": [
      "default",
      {
        "aria-command-name": "off",
        "aria-progressbar-name": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "meta-viewport": "off"
  }
}